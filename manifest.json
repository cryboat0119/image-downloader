{
  "name": "Image Downloader",
  "description": "See all images shown or linked on a web page and download them.",
  "version": "1.0.13",
  "manifest_version": 2,
  "permissions": [
    "tabs", "http://*/*", "https://*/*"
  ],
  "browser_action": {
    "default_icon": "icon_128.png",
    "default_popup": "popup.html"
  },
  "icons": {
    "16": "icon_16.png",
    "48": "icon_48.png",
    "128": "icon_128.png"
  },
  "background": {
    "scripts": ["defaults.js"]
  },
  "options_page": "options.html"
}
