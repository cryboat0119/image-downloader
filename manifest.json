{
  "name": "Image Downloader",
  "description": "Displays all images on a web page and allows the user to choose which ones to download.",
  "version": "1.0.7",
  "manifest_version": 2,
  "permissions": [
    "tabs", "http://*/*", "https://*/*"
  ],
  "browser_action": {
    "default_icon": "icon_128.png",
    "default_popup": "popup.html"
  },
  "icons": {
    "16": "icon_16.png",
    "48": "icon_48.png",
    "128": "icon_128.png"
  },
  "web_accessible_resources": [
    "icon_48.png"
  ],
  "background": {
    "scripts": ["defaults.js"]
  },
  "options_page": "options.html"
}
