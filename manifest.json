{
  "name": "Image Downloader",
  "description": "Displays all images on a web page and allows the user to choose which ones to download.",
  "version": "1.0",
  "manifest_version": 2,
  "permissions": [
    "tabs", "http://*/*", "https://*/*"
  ],
  "browser_action": {
    "default_icon": "icon.png",
    "default_popup": "popup.html"
  },
  "background": {
    "scripts": ["defaults.js"]
  },
  "options_page": "options.html"
}
